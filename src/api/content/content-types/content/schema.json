        "images"
      ]
    },
    "document": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "editor": {
      "type": "richtext"
    },
    "approval": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::approval.approval",
      "inversedBy": "content"
    },
    "approved": {
      "type": "boolean",
      "default": false
    },
    "novi": {
      "type": "json",
      "private": true
